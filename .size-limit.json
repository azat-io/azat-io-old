[
  {
    "name": "Main script",
    "path": [
      "dist/assets/chunks/framework.*.js",
      "dist/assets/chunks/module.*.js",
      "dist/assets/app.*.js"
    ],
    "limit": "40 kB",
    "brotli": true
  },
  {
    "name": "All scripts",
    "path": ["dist/assets/**/*.js"],
    "limit": "140 kB",
    "brotli": true
  },
  {
    "name": "All styles",
    "path": ["dist/assets/*.css"],
    "limit": "10 kB",
    "brotli": true
  }
]
